<% provide(:title, @user['name']) %>
<h1><%=@user['name']%></h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Job Title</th>
      <th>Business Unit</th>
      <th>Department</th>
      <th>Location</th>
      <th>Hire Date</th>
      <th>Manager Name</th>
    </tr>
  </thead>
  <tbody>
    <%= render :partial => 'user', :locals => {:user => @user} %>
  </tbody>
</table>
